<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only">
  <info>
    <title>Inline Round Bracket Style</title>
    <id>http://www.zotero.org/styles/inline-round-bracket</id>
    <link href="http://www.zotero.org/styles/inline-round-bracket" rel="self"/>
    <link href="http://www.zotero.org/styles/harvard-cite-them-right" rel="template"/>
    <author>
      <name>ChatGPT</name>
    </author>
    <category citation-format="author-date"/>
    <updated>2023-06-10T00:00:00+00:00</updated>
  </info>
  <macro name="author">
    <names variable="author" et-al-min="2" et-al-use-first="1" et-al-subsequent-min="2" et-al-subsequent-use-first="1">
      <name form="short" and="symbol" delimiter-precedes-last="never" initialize-with=". " name-as-sort-order="all"/>
      <label form="short" prefix=" (" suffix=")"/>
      <et-al font-style="italic"/>
    </names>
  </macro>
  <macro name="year-date">
    <date variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <macro name="container-title">
    <text variable="container-title"/>
  </macro>
  <macro name="volume">
    <text variable="volume"/>
  </macro>
  <macro name="page">
    <text variable="page"/>
  </macro>
  <citation>
    <layout prefix="(" suffix=")">
      <group delimiter=", ">
        <text macro="author"/>
        <text macro="container-title"/>
        <text macro="volume"/>
        <text macro="page"/>
        <text macro="year-date"/>
      </group>
    </layout>
  </citation>
  <bibliography>
    <layout>
      <group delimiter=", ">
        <text macro="author"/>
        <text macro="container-title"/>
        <text macro="volume"/>
        <text macro="page"/>
        <text macro="year-date"/>
      </group>
    </layout>
  </bibliography>
</style>
